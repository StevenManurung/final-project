<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/darkblue"
            app:popupTheme="@style/Alamat_user.PopupOverlay"
            app:title="Alamat Lengkap"
            app:titleTextColor="#fafafa" />

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="56dp"
        android:padding="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="Nama"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_nama"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="Steven Manurung"
            android:textSize="17sp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textView12" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="No Hp"
            app:layout_constraintTop_toBottomOf="@+id/tv_nama"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_nohp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="123123"
            android:textSize="17sp"
            app:layout_constraintEnd_toEndOf="@id/tv_nama"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/tv_nama"
            app:layout_constraintTop_toBottomOf="@+id/textView11" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="Provinsi"
            app:layout_constraintTop_toBottomOf="@+id/tv_nohp"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_provinsi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="Sumatera Utara"
            android:textSize="17sp"
            app:layout_constraintEnd_toEndOf="@id/tv_nohp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/tv_nohp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="Kota"
            app:layout_constraintTop_toBottomOf="@+id/tv_provinsi"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_kota"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="Medan"
            android:textSize="17sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/tv_provinsi"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="Kecamatan"
            app:layout_constraintTop_toBottomOf="@+id/tv_kota"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_kecamatan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="Medan Perjuangan"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/tv_kota"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="Kode Pos"
            app:layout_constraintTop_toBottomOf="@+id/tv_kecamatan"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_kodepos"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="20131"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@id/tv_kecamatan"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="@string/nama_jalan_gedung_no_rumah"
            app:layout_constraintTop_toBottomOf="@+id/tv_kodepos"
            tools:layout_editor_absoluteX="10dp" />

        <TextView
            android:id="@+id/tv_namajalan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"
            android:text="Jl Perjuangan no 321"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/tv_kodepos"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/poppinsbold"
            android:paddingBottom="5dp"
            android:text="@string/detail_lainnya_cth_blok_unit_no_patokan"
            app:layout_constraintTop_toBottomOf="@+id/tv_namajalan"
            tools:layout_editor_absoluteX="16dp" />

        <TextView
            android:id="@+id/tv_detailalamat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppinsbold"

            android:text="sfkdjafklsajfas ksdfnkasnfklsanfs fnkalsdfnlsda fnlks adnflkasnlkf nslkdf nlksan flknsaldf nsadlfn lsndlkfn lsandlf aksdkdnfksndkfnksdnfna;lsdfna sdnflas;d nflasdnflasdnflasndfoinaoi fnslkdfna'"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@id/tv_namajalan"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <Button
            android:id="@+id/btn_ubah"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="50dp"
            android:background="@drawable/rounded_button_ubah"
            android:fontFamily="@font/poppinsbold"
            android:text="Ubah"
            android:textColor="@color/white"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="@id/tv_detailalamat"
            app:layout_constraintEnd_toStartOf="@id/btn_hapus"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_detailalamat" />

        <Button
            android:id="@+id/btn_hapus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/rounded_button_hapus"
            android:fontFamily="@font/poppinsbold"
            android:text="Hapus"
            android:textColor="@color/white"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/btn_ubah"
            app:layout_constraintTop_toBottomOf="@id/tv_detailalamat" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>